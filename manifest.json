{
    "name": "bear",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action":{
        "default_title": "Bear"
    },
    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "*://*/*"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "web_accessible_resources": [
        "subtitle.ytt"
     ]
}

